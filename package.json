{
  "name": "substrate-front-end-template",
  "version": "0.1.0",
  "private": true,
  "author": "Selendra",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clean": "polkadot-dev-clean-build",
    "test": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js",
    "lint:ci": "eslint src/**/*.js --max-warnings=0",
    "lint:fix": "eslint --fix src/**/*.js",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
  },
  "dependencies": {
    "@polkadot/api": "6.7.2",
    "@polkadot/extension-dapp": "0.41.1",
    "@polkadot/ui-keyring": "0.86.5",
    "@polkadot/util": "7.8.2",
    "@polkadot/util-crypto": "7.8.2",
    "ethers": "^5.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.2.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^4.0.3",
    "react-select": "^5.2.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@polkadot/dev": "^0.65.23",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-semistandard": "^16.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-promise": "^5.1.1",
    "gh-pages": "^3.2.3",
    "process": "^0.11.10",
    "semistandard": "^16.0.0",
    "url-loader": "^4.1.1"
  },
  "resolutions": {
    "eslint-plugin-jsx-a11y": "6.4.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "semistandard"
    ],
    "plugins": [
      "only-warn"
    ]
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
